                            </li>
                            <li>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
            <div id="Catogary1" class="CatogariesLnk">
                <ul>
                    <li>
                        @*<a href="#">Pakistani</a>*@
                        @Html.ActionLink("Pakistani", "Index", "Dashboard")
                        <ul class="SubCatogaries">
                            <li>
                                @*<a href="#">Fast Food</a>*@
                                @Html.ActionLink("Fast Food", "Index", "Dashboard")
                            </li>
                            <li>
                                @*<a href="#">Desi</a>*@
                                @Html.ActionLink("Desi", "Index", "Dashboard")
                            </li>
                            <li>
                                @*<a href="#">Lite</a>*@
                                @Html.ActionLink("Lite", "Index", "Dashboard")
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
            <div id="Catogary2" class="CatogariesLnk">
                <ul>
                    <li>
                        @*<a href="#">Chinese</a>*@
                        @Html.ActionLink("Chinese", "Index", "Dashboard")
                        <ul class="SubCatogaries">
                            <li>
                                @*<a href="#">Fast Food</a>*@
                                @Html.ActionLink("Fast Food", "Index", "Dashboard")

                            </li>
                            <li>
                                @*<a href="#">Desi</a>*@
                                @Html.ActionLink("Desi", "Index", "Dashboard")
                            </li>
                            <li>
                                @*<a href="#">Lite</a>*@
                                @Html.ActionLink("Lite", "Index", "Dashboard")
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
            <div id="Catogary3" class="CatogariesLnk">
                <ul>
                    <li>
                        @*<a href="#">Continental</a>*@
                        @Html.ActionLink("Continental", "Index", "Dashboard")
                        <ul class="SubCatogaries">
                            <li>
                                @*<a href="#">Fast Food</a>*@
                                @Html.ActionLink("Fast Food", "Index", "Dashboard")
                            </li>
                            <li>
                                @*<a href="#">Desi</a>*@
                                @Html.ActionLink("Desi", "Index", "Dashboard")
