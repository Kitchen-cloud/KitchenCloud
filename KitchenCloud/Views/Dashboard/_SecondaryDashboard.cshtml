


<!DOCTYPE html>
<html>
<head>
    <title>
        @ViewBag.Title
    </title>
    <meta charset="utf-8" />
    <link href="~/Content/Styles/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet"/>
    <link rel="stylesheet" type="text/css" href="~/Content/sliderengine/amazingslider-1.css">


    <style>
        a:focus, :focus {
            outline: none;
        }

        input:focus {
            outline: none;
        }

        .fixed {
            position: fixed;
            top: -1px;
            z-index: 1;
        }

        .SecondaryNav {
            background-color: #EEEEEE;
            height: 30px;
            padding-left: 50px;
        }

            .SecondaryNav ul li {
                float: left;
                display: block;
                margin-right: 60px;
                padding-top: 4px;
            }

                .SecondaryNav ul li a {
                    color: gray;
                }

        .lbl {
            font-size: 12px;
            color: lightgray;
        }

        .txtbx {
            color: steelblue;
        }
    </style>
</head>
<body>



    <div class="container-fluid">

        <!--Header Area Reserved-->
        <div style="width: 100%; position: fixed; z-index: 3;" id="navbars">
            @RenderPage("_PrimaryNavBar.cshtml")
            @RenderPage("_SecondaryNavBar.cshtml")
        </div>
        <br /><br />
        <br /><br />
        <br />
        <br />








        <div class="container-fluid">

            <div class="col-lg-12 col-md-8 col-sm-12 col-xs-12">
               <br />
                <div class="row">
                    @RenderBody()
                </div>
            </div>
        </div>

        <br /><br /><br /><br /><br /><br /><br />
      

        @RenderPage("_LogIn.cshtml")
        @*@RenderPage("_RecoverPassword.cshtml")*@
        @RenderPage("_SignUp.cshtml")


        <!--Forms-->





    </div>
    @RenderPage("~/Views/Shared/_GlobalFooter.cshtml")
    <div class="modal"><!-- Place at bottom of page --></div>


<script src="~/Scripts/jquery-2.2.3.min.js"></script>
    @RenderSection("RecipeDescription",false)
    <script src="~/Scripts/jquery-ui.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>

    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/Index.js"></script>



</body>

</html>